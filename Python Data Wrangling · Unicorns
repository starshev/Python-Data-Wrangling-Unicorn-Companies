     "execution_count": 109,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "national_valuations = df_companies.groupby(\n",
    "    'Country/Region')['valuation_num'].sum().sort_values(ascending = False).reset_index()\n",
    "\n",
    "national_valuations.head(15)"
   ]
